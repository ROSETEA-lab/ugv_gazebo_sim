    <plugin name='gazebo_ros_control' filename='libgazebo_ros_control.so'>
      <robotNamespace>/</robotNamespace>
    </plugin>
    <static>0</static>

    <plugin name="scout_mini_joint_state" filename="libgazebo_ros_joint_state_publisher.so">
      <ros>
        <!-- <namespace>/tb3</namespace> -->
        <remapping>~/out:=joint_states</remapping>
      </ros>
      <update_rate>50</update_rate>
      <joint_name>front_right_wheel</joint_name>
      <joint_name>front_left_wheel</joint_name>
      <joint_name>rear_right_wheel</joint_name>
      <joint_name>rear_left_wheel</joint_name>
    </plugin>      

    <plugin name='skid_steer_drive' filename='libgazebo_ros_diff_drive.so'>
      <!-- topics & remappings -->
      <ros>
        <namespace>/</namespace>
        <argument>cmd_vel:=cmd_vel</argument>
        <argument>odom:=odom</argument>
      </ros>
        
      <cmd_vel_timeout>0.25</cmd_vel_timeout>
      <velocity_rolling_window_size>0.1</velocity_rolling_window_size>

      <!-- Number of wheel pairs -->
      <num_wheel_pairs>2</num_wheel_pairs>
        
      <!-- front wheels -->
      <left_joint>front_left_wheel</left_joint>
      <right_joint>front_right_wheel</right_joint>

      <!-- rear wheels-->
      <left_joint>rear_left_wheel</left_joint>
      <right_joint>rear_right_wheel</right_joint>

      <!-- kinematics -->
      <wheel_separation>0.490</wheel_separation>
      <wheel_diameter>0.160</wheel_diameter>
        
      <wheel_separation_multiplier>1.0</wheel_separation_multiplier>
      <left_wheel_radius_multiplier>1.0</left_wheel_radius_multiplier>
      <right_wheel_radius_multiplier>1.0</right_wheel_radius_multiplier>
        
      <!-- Velocity and acceleration limits -->
      <linear>
         <x>
            <has_velocity_limits>true</has_velocity_limits>
            <max_velocity>2.0</max_velocity>
            <has_acceleration_limits>true</has_acceleration_limits>
            <max_velocity>3.0</max_velocity>
         </x>
      </linear>
      <angular>
         <z>
            <has_velocity_limits>true</has_velocity_limits>
            <max_velocity>2.0</max_velocity>
            <has_acceleration_limits>true</has_acceleration_limits>
            <max_velocity>6.0</max_velocity>
         </z>
      </angular>
       
      <!-- output -->
      <enable_odom_tf>false</enable_odom_tf>
      
      <update_rate>50.0</update_rate>
        
      <pose_covariance_diagonal>[0.001, 0.001, 0.001, 0.001, 0.001, 0.03]</pose_covariance_diagonal>
      <twist_covariance_diagonal>[0.001, 0.001, 0.001, 0.001, 0.001, 0.03]</twist_covariance_diagonal>

      <!-- odometry -->
      <odometry_frame>odom</odometry_frame>
      <robot_base_frame>base_link</robot_base_frame>
    </plugin>
